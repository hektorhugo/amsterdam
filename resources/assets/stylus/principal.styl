@import "backgroundImages"
& > *
       margin 0
       padding 0

html,body
   height 100%
   color #fff
   -moz-background-size cover
   -webkit-background-size cover
   -o-background-size cover
   background-size cover
   background url("../image/body-bg.jpg") no-repeat center center fixed

#wrapper
   min-height 100%


section
   overflow auto
   padding-bottom 69px
   padding-top 30px

footer
   position relative
   margin-top -69px
   height 69px
   padding 5px 0px
   clear both
   //background #00a1de
   //opacity  0.8
   background-color: rgba(0, 161, 222, 0.8);
   text-align center
   color #fff

.define
   max-width 960px
   margin 0 auto

.navbar
  //background #00a1de
  background-color: rgba(0, 161, 222, 0.8);
  border none
  color #fff

 .navbar-default
     .navbar-nav
         & > li
             & > a
                 color #fff
.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active > a:hover, .navbar-default .navbar-nav > .active > a:focus
   background-color #2cc5ff
   color #000

.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a:focus
   background-color #2cc5ff
   color #000

.logo_blanco
     -webkit-filter invert(100%)
     filter invert(100%)
.blanco
  border-image url("../image/borde.png") 33% round;
  -moz-border-image url("../image/borde.png") 33% round;
  -webkit-border-image url("../image/borde.png") 33% round;
  -o-border-image url("../image/borde.png") 33% round;
  background-color #fff
  color #000
.vertical-align
    display flex
    flex-direction row

.vertical-align
  display: flex
  align-items: center
.fondo_negro
  background-color: rgba(0,0,0,0.6)
  padding-top: 5px;
  padding-right: 5px;
  padding-bottom: 5px;
  padding-left: 5px;
.center_image
  display block
  margin-left auto
  margin-right auto
.fondo_div
  background url("../image/div.png") no-repeat
  color #000
  .row
    margin-left: 15px;
@media (max-width: 768px)
  .navbar
    //background #00a1de
    background-color: rgba(0, 161, 222, 0);
  .navbar-toggle
    background-color: rgba(0, 161, 222, 0.8);
  .navbar-collapse
    background-color: rgba(0, 161, 222, 0.8);
  .logo_blanco
    max-height: 64px;
